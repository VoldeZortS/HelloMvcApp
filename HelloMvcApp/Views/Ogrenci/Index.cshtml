@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <ul>
        @for (int i = 0; i < 5; i++)
        {
            <li>@i</li>
        }
    </ul>
    <table border="1">
        @for (int i = 0; i < 5; i++)
        {
           <tr>
                   <td>@i</td>
           </tr>
        }
    </table>
</body>
</html>
